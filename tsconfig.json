{
    "compilerOptions": {
        "target": "es5",
        "downlevelIteration": true,
        "lib": ["dom", "dom.iterable", "esnext", "es5", "es6"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "jsxImportSource": "@emotion/react",
        "baseUrl": ".",
        // "types": ["@emotion/react/types/css-prop", "node"],
        "typeRoots": ["./node_modules/@types/", "./assets/react/@types"],
        "paths": {
            "@Atoms/*": ["assets/react/v3/atoms/*"],
            "@Molecules/*": ["assets/react/v3/molecules/*"],
            "@Components/*": ["assets/react/v3/components/*"],
            "@Config/*": ["assets/react/v3/config/*"],
            "@Hooks/*": ["assets/react/v3/hooks/*"],
            "@Utils/*": ["assets/react/v3/utils/*"],
            "@CBComponents/*": ["assets/react/v3/course-builder/components/*"],
            "@CBServices/*": ["assets/react/v3/course-builder/services/*"],
        }
    },
    "include": [
        "./assets/react"
    ],
    "exclude": [
        "node_modules"
    ]
}